<header odom-ml="xml" html="header">
  <picture-area class="picture-area">
    <contacts class="primary" odom-map='{"data": "@data.primaryContacts", "createNode": "@methods.createLink"}'></contacts>
    <pyramid odom-src="pyramid"></pyramid>
    <contacts class="secondary" odom-map='{"data": "@data.secondaryContacts", "createNode": "@methods.createLink"}'></contacts>
  </picture-area>

  <brand odom-src="brand"></brand>
</header>